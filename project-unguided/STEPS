cd project-unguided-112683104/
wget http://nlp.stanford.edu/~socherr/stanfordSentimentTreebank.zip
unzip stanfordSentimentTreebank.zip

python -c "from data import dataReader; dr = dataReader(); dr.to_csv('./');"
python2.7 csv2mySQL.py text.csv sentiment SST_posts '(message_id int(10) PRIMARY KEY, message text)' 1
python2.7 csv2mySQL.py outcomes.csv sentiment SST_outcomes '(message_id int(10) PRIMARY KEY, coarse_label int(10), fine_label int(10), train_test_eval_flag int(10))' 1

python3 dlatkInterface.py -d sentiment -t SST_posts -c message_id --add_ngrams -n 1 2 --combine_feat_tables 1to2gram
python3 dlatkInterface.py -d sentiment -t SST_posts -c message_id --add_ngrams -n 1 2 3 --combine_feat_tables 1to3gram